<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0">
    <description>Aggregates plugins providing scanners and rules for Java EE 6 technologies (e.g. JPA2, EJB3).</description>
    <model>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.WarArchiveDescriptor</class>
        <class>com.buschmais.jqassistant.plugin.javaee6.api.model.EarArchiveDescriptor</class>
    </model>
    <scanner>
        <class>com.buschmais.jqassistant.plugin.javaee6.impl.scanner.EarArchiveScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>